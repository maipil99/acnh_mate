pipeline {
    agent any
    parameters{
        run filter: 'SUCCESSFUL', name: 'PROMOTED_BUILD', projectName: 'ACNH Mate'
    }
    stages {
        stage("Deploy") {
            steps{
                sh "docker-compose --env-file config/Prod.env pull"
                sh "docker-compose --env-file config/Prod.env up -d"
            }
        }
    }
}